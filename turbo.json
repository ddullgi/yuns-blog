{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [
				"dist/**",
				".next/**",
				"!.next/cache/**",
				"storybook-static/**"
			]
		},
		"lint": {
			"outputs": []
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
